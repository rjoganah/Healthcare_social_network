@font-face {
    font-family: basic;
    src: url('/font/basictitlefont.ttf');
}

body {
    margin:0;
    font-family: Helvetica, sans-serif;
    /*background-image:url(img_global/wall1.png);*/
    background-color: gainsboro;
    font-size:15px;
    color:#000;
}

#all-content {
    width:960px;
    margin-left:auto;
    margin-right:auto;
}

/*#main-statebar {
    border:2px solid #3b3938;
    border-top:1px solid #000000;
}*/

#header {
    height:120px;
    margin-top:1%;
    margin-bottom:0%;
    background-image: url(script/img/banniere2.png);
}

#header:hover {
    background-image: url(script/img/banniere2.png);
}


#footer {
    height:45px;
    margin-top:1%;
    margin-bottom:1%;
    text-align: center;
}

#dropdownsignin {
    z-index: 2;
    float: right;
    display : none;
    text-decoration: none;
    margin-top: 5px;
    margin-right: -25px;
    position: relative;
}

#dropdownsignin table {
    background-color: dodgerblue;
    border-radius: 0px;
}

.jcarousel {
    z-index: 1;
    box-shadow:1px 1px 10px #000000;
}

#statebar {
    margin-bottom:-1%;
    position : relative;
    margin-left: 10px;
    margin-right:10px;
    height:10px;
    /*background-image:url(img_global/wall2.png);
    padding-right: 20px;*/
    margin-top:0px;
    padding-top:10px;
    padding-bottom: 10px;
    background-color: rgba(0,0,0,0.8);
    border-radius: 0 0 10px  10px;
    text-align: right;
/*    border-bottom: 4px solid #000000;*/
}

#statebar ul {
    /*    position: relative;
        padding: 0px;*/
    padding-right: 20px;
    margin:0;
}

#statebar ul li {
    display: inline;
    text-decoration: none;
}

#statebar a {
    color: #000;
    font-weight:bold;
    text-decoration: none;
}

#statebar a:hover {
    text-decoration: underline;
}

div #footer,#header {
    /*background-image:url(img_global/wall2.png);*/
    background-color: ghostwhite;
    border-left: 1px solid darkgrey;
    border-right: 1px solid darkgrey;
    border-top: 1px solid darkgrey;
    border-bottom: 1px solid darkgrey;
    padding:1em;
}

div #main {
    background-color: ghostwhite;
    padding:1em;
    
}

