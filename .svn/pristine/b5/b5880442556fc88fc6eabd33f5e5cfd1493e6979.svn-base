input {
    border: 2px solid #3b3938;
    border-radius:2px;
}

textarea {
    border: 2px solid #3b3938;
    border-radius:2px;
}

.correct-field {
    border-color: #3b3938;
}

.incorrect-field {
    border-color: red;
}

.print-message-form {
    font-weight:bold;
    text-align: center;
    padding: 3px;
    margin-bottom: 10px;
    border:2px solid red;
    border-radius:5px;
    background-color: rgba(255, 0, 0, 0.25);
}

#info-user-profile {
    height: 140px;
    /*    background-color: rgb(255, 0, 0);*/
    margin-left:60%;
}

#info-user-profile div {
    float: right;    
}

#info-user-profile div table {
    border: 0px;
    text-align: left;
    margin-right: 40px;
    margin-top: 10px;
}

#info-exam-profile table {
    width: 600px;
    margin-right:30%;
    margin-left: auto;
}
